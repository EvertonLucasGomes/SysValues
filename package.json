{
  "name": "sistema-de-gestao-agropecuaria",
  "version": "1.0.0",
  "description": "> Guia completo para configurar o ambiente local com Git, SSH e clonar o reposit√≥rio usando Windows",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "dev:frontend": "npm run dev --workspace=apps/frontend",
    "dev:backend": "npm run dev --workspace=apps/backend",
    "dev": "concurrently -k -n FRONTEND,BACKEND -c blue,green \"npm run dev:frontend\" \"npm run dev:backend\"",
    "setup": "npm install && npm run format && npm run lint && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarceloDChagas/Sistema-de-gestao-agropecuaria.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarceloDChagas/Sistema-de-gestao-agropecuaria/issues"
  },
  "homepage": "https://github.com/MarceloDChagas/Sistema-de-gestao-agropecuaria#readme",
  "devDependencies": {
    "@nestjs/testing": "^10.4.19",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "^30.0.0",
    "lint-staged": "13.3.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "prettier": "2.8.8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "turbo": "1.12.4",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^7.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
